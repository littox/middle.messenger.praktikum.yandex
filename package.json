{
  "name": "m1",
  "scripts": {
    "dev": "npm run build:dev && webpack serve --mode development --port 3000",
    "build:dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "npm run build && node server.js",
    "eslint": "npx eslint .",
