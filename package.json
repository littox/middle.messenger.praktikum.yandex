    "@typescript-eslint/parser": "5.45.0",
    "chai": "4.3.7",
    "css-loader": "6.7.3",
    "eslint": "8.28.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "handlebars": "4.7.7",
    "handlebars-loader": "1.7.3",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "mini-css-extract-plugin": "2.7.2",
